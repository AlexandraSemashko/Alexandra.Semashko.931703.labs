﻿@{
    ViewData["Title"] = "PassUsingViewData";
    var perem = ViewData["Class"] as Maths;
}
<h2> @ViewData["Title"]</h2>

<h5><b>Rand Value First :</b> @perem.x</h5>
<h5><b>Rand Value Second :</b> @perem.y</h5>
<h5><b>Add :</b><br> @perem.x + @perem.y = @perem.Add()</h5>
<h5><b>Sub :</b><br> @perem.x - @perem.y = @perem.Sub()</h5>
<h5><b>Mult :</b><br> @perem.x * @perem.y = @perem.Mult()</h5>
<h5><b>Div :</b><br> @perem.x / @perem.y = @perem.Div()</h5>

