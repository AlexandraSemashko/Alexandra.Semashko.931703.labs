@{
    @inject IPodMaths PodMaths;
    @PodMaths.Nov();
 }

<h2>@ViewData["Title"]</h2>
<h5><b>Rand Value First :</b> @PodMaths.tec.x</h5>
<h5><b>Rand Value Second :</b> @PodMaths.tec.y</h5>
<h5><b>Add :</b><br>@PodMaths.tec.x + @PodMaths.tec.y = @PodMaths.tec.Add()</h5>
<h5><b>Sub :</b><br>@PodMaths.tec.x - @PodMaths.tec.y = @PodMaths.tec.Sub()</h5>
<h5><b>Mult :</b><br>@PodMaths.tec.x * @PodMaths.tec.y = @PodMaths.tec.Mult()</h5>
<h5><b>Div :</b><br>@PodMaths.tec.x / @PodMaths.tec.y = @PodMaths.tec.Div()</h5>

